<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gdou.mapper.SalaryItemMapper">

    <select id="selectCountBySalaryItemId" resultType="java.lang.Integer">
        select count(*)
        from salary_standard_item
        where salary_item_id = #{salaryItemId}
    </select>
</mapper>
